<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:AvaGodots"
             x:Class="AvaGodots.App"
             RequestedThemeVariant="Dark">

  <Application.DataTemplates>
    <local:ViewLocator/>
  </Application.DataTemplates>

  <Application.Styles>
    <FluentTheme/>
    <StyleInclude Source="avares://AvaGodots/Resources/GodotsTheme.axaml"/>
  </Application.Styles>

  <Application.Resources>
    <ResourceDictionary>
      <ResourceInclude x:Key="L10N.en" Source="avares://AvaGodots/Resources/en.axaml"/>
      <ResourceInclude x:Key="L10N.zh-CN" Source="avares://AvaGodots/Resources/zh-CN.axaml"/>
    </ResourceDictionary>
  </Application.Resources>

</Application>
